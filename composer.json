{
    "name": "inweb/admin-permissions-tool",
    "description": "A Laravel Admin tool.",
    "keywords": [
        "laravel",
        "admin"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Inweb\\Tools\\PermissionsTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inweb\\Tools\\PermissionsTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
